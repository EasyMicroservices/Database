<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFrameworks>net6.0;</TargetFrameworks>
		<Platforms>AnyCPU;x64;x86</Platforms>
		<Authors>EasyMicroservices</Authors>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<Version>0.0.0.1</Version>
		<Description>Manage your files with a wrapper in everywhere.</Description>
		<Copyright>EasyMicroservices@gmail.com</Copyright>
		<PackageTags>database</PackageTags>
		<PackageProjectUrl>https://github.com/EasyMicroservices/Database</PackageProjectUrl>
		<LangVersion>latest</LangVersion>
		<TreatWarningsAsErrors>true</TreatWarningsAsErrors>
		<DocumentationFile>.\bin\$(Configuration)\$(TargetFramework)\EasyMicroservices.Database.EntityFrameworkCore.PostgreSQL.xml</DocumentationFile>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="Npgsql.EntityFrameworkCore.PostgreSQL" Version="7.0.1" />
	</ItemGroup>

	<ItemGroup>
		<ProjectReference Include="..\EasyMicroservices.Database\EasyMicroservices.Database.csproj" />
	</ItemGroup>

</Project>
